{
  "name": "@rznewswav/gh-npm-package",
  "version": "1.0.1-fix.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && swc src -s -d ./dist",
    "test": "APP_ENV=testing jest",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rznewswav/gh-npm-package.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rznewswav/gh-npm-package/issues"
  },
  "homepage": "https://github.com/rznewswav/gh-npm-package#readme",
  "engines": {
    "node": ">=16.x.x",
    "npm": ">=8.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.2.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "prettier": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "typescript": "^4.9.3"
  },
  "optionalDependencies": {
    "reflect-metadata": "^0.1.13"
  }
}